<%- model_class = Meme -%>

<div class="page-header" >

  <% @memes.each do |meme| %>
  <p>
    <h1> <%= meme.name %></h1>
    <%= link_to image_tag(meme.image.url(:medium)), meme_path(meme) %>
  </p>
  </br>
  <% end %>

</div>


<%= link_to t('.new', :default => t("helpers.links.new")),
            new_meme_path,
            :class => 'btn btn-primary' %>
